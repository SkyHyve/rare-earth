{
  "name": "@rare-earth/react-tables",
  "version": "0.4.5",
  "description": "A feature-rich, lightweight React table component with advanced sorting, filtering, pagination, and data export capabilities.",
  "main": "./dist/bundle.cjs",
  "module": "./dist/bundle.esm",
  "browser": "./dist/bundle.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/bundle.esm",
      "require": "./dist/bundle.cjs",
      "browser": "./dist/bundle.js"
    }
  },
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist/",
    "README.md",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SkyHyve/rare-earth.git"
  },
  "keywords": [
    "react",
    "table",
    "datatable",
    "grid",
    "sorting",
    "filtering",
    "pagination",
    "export",
    "csv",
    "typescript",
    "component",
    "data-grid",
    "ui",
    "frontend"
  ],
  "author": "Matthew LW Graham",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SkyHyve/rare-earth/issues"
  },
  "homepage": "https://github.com/SkyHyve/rare-earth#readme",
  "dependencies": {
    "prop-types": "15.8.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-icons": "5.5.0",
    "throttle-debounce": "5.0.2",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@babel/core": "7.28.3",
    "@babel/preset-typescript": "7.27.1",
    "@babel/plugin-transform-react-jsx": "7.27.1",
    "@babel/preset-env": "7.28.3",
    "@babel/preset-react": "7.27.1",
    "@babel/runtime": "7.28.3",
    "@floating-ui/react": "0.27.16",
    "@rollup/plugin-alias": "5.1.1",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-commonjs": "28.0.6",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@rollup/plugin-replace": "6.0.2",
    "@rollup/plugin-typescript": "12.1.4",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "nodemon": "3.1.10",
    "rollup": "4.47.1",
    "rollup-plugin-postcss": "4.0.2",
    "tslib": "2.8.1",
    "typescript": "5.9.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
